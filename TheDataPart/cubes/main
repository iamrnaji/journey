
import java.util.Scanner;

public class Cubes {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        while(true)
        {
            String num = scanner.nextLine();
            if(num.contains("end"))
            {
                break;
            }
            int value = Integer.valueOf(num);
            value = value * value * value; 
            System.out.println(value);
        }

    }
}
